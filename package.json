{
  "name": "interpreter-context-jsdom",
  "version": "1.0.0",
  "description": "DOM-like context for shift-interpreter",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "compile": "npm run clean && tsc --declaration",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run compile",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "watch": "tsc -w",
    "test": "mocha"
  },
  "keywords": [
    "shift-interpreter",
    "context",
    "jsdom"
  ],
  "author": "Jarrod Overson <jsoverson@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/jsdom": "^16.2.3",
    "atob": "^2.1.2",
    "btoa": "^1.2.1",
    "jsdom": "^16.3.0",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "@types/atob": "^2.1.2",
    "@types/btoa": "^1.2.3",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.0",
    "@types/urlencode": "^1.1.2",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
